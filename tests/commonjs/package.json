{
  "name": "commonjs",
  "version": "1.0.0",
  "main": "lib/main.cjs",
  "license": "MIT",
  "private": true,
  "scripts": {
    "package": "ncc build --source-map --license licenses.txt -m -q",
    "test": "jest --coverage --silent=false --verbose false",
    "build": "tsc"
  },
  "type": "commonjs",
  "devDependencies": {
    "@swc/core": "1.3.11",
    "@types/jest": "^29.5.13",
    "@types/node": "^20.16.5",
    "@vercel/ncc": "^0.36.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "appstore-connect-jwt-generator-core": "link:../../package"
  },
  "jest": {
    "testEnvironment": "node",
    "clearMocks": true,
    "extensionsToTreatAsEsm": [
      ".ts",
      ".cts"
    ],
    "moduleFileExtensions": [
      "cts",
      "ts",
      "js",
      "cjs",
      "json",
      "node"
    ],
    "testMatch": [
      "**/__tests__/*.test.ts",
      "**/__tests__/*.test.cts"
    ],
    "transform": {
      "^.+\\.c?(t|j)sx?$": [
        "ts-jest/legacy"
      ]
    },
    "modulePaths": [
      "<rootDir>/src",
      "<rootDir>/dist",
      "<rootDir>/node_modules"
    ],
    "verbose": true
  },
  "engines": {
    "npm": "use pnpm please!",
    "yarn": "use pnpm please!",
    "pnpm": ">=8",
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@9.10.0+sha512.73a29afa36a0d092ece5271de5177ecbf8318d454ecd701343131b8ebc0c1a91c487da46ab77c8e596d6acf1461e3594ced4becedf8921b074fbd8653ed7051c"
}
