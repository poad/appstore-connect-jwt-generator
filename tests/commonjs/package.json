{
  "name": "commonjs",
  "version": "1.0.0",
  "main": "lib/main.cjs",
  "license": "MIT",
  "private": true,
  "scripts": {
    "package": "ncc build --source-map --license licenses.txt -m -q",
    "test": "jest --coverage --silent=false --verbose false",
    "build": "tsc"
  },
  "type": "commonjs",
  "devDependencies": {
    "@swc/core": "1.3.11",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.10",
    "@vercel/ncc": "^0.36.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.5",
    "typescript": "^5.5.3"
  },
  "dependencies": {
    "appstore-connect-jwt-generator-core": "link:../../package"
  },
  "jest": {
    "testEnvironment": "node",
    "clearMocks": true,
    "extensionsToTreatAsEsm": [
      ".ts",
      ".cts"
    ],
    "moduleFileExtensions": [
      "cts",
      "ts",
      "js",
      "cjs",
      "json",
      "node"
    ],
    "testMatch": [
      "**/__tests__/*.test.ts",
      "**/__tests__/*.test.cts"
    ],
    "transform": {
      "^.+\\.c?(t|j)sx?$": [
        "ts-jest/legacy"
      ]
    },
    "modulePaths": [
      "<rootDir>/src",
      "<rootDir>/dist",
      "<rootDir>/node_modules"
    ],
    "verbose": true
  },
  "engines": {
    "npm": "use pnpm please!",
    "yarn": "use pnpm please!",
    "pnpm": ">=8",
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@9.5.0+sha512.140036830124618d624a2187b50d04289d5a087f326c9edfc0ccd733d76c4f52c3a313d4fc148794a2a9d81553016004e6742e8cf850670268a7387fc220c903"
}
