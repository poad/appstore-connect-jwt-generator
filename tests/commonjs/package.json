{
  "name": "commonjs",
  "version": "1.0.0",
  "main": "lib/main.cjs",
  "license": "MIT",
  "private": true,
  "scripts": {
    "package": "ncc build --source-map --license licenses.txt -m -q",
    "test": "jest --coverage --silent=false --verbose false",
    "build": "tsc"
  },
  "type": "commonjs",
  "devDependencies": {
    "@swc/core": "1.3.11",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.16.2",
    "@vercel/ncc": "^0.36.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "appstore-connect-jwt-generator-core": "link:../../package"
  },
  "jest": {
    "testEnvironment": "node",
    "clearMocks": true,
    "extensionsToTreatAsEsm": [
      ".ts",
      ".cts"
    ],
    "moduleFileExtensions": [
      "cts",
      "ts",
      "js",
      "cjs",
      "json",
      "node"
    ],
    "testMatch": [
      "**/__tests__/*.test.ts",
      "**/__tests__/*.test.cts"
    ],
    "transform": {
      "^.+\\.c?(t|j)sx?$": [
        "ts-jest/legacy"
      ]
    },
    "modulePaths": [
      "<rootDir>/src",
      "<rootDir>/dist",
      "<rootDir>/node_modules"
    ],
    "verbose": true
  },
  "engines": {
    "npm": "use pnpm please!",
    "yarn": "use pnpm please!",
    "pnpm": ">=8",
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@9.9.0+sha512.60c18acd138bff695d339be6ad13f7e936eea6745660d4cc4a776d5247c540d0edee1a563695c183a66eb917ef88f2b4feb1fc25f32a7adcadc7aaf3438e99c1"
}
