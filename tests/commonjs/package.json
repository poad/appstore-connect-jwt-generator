{
  "name": "commonjs",
  "version": "1.0.0",
  "main": "lib/main.cjs",
  "license": "MIT",
  "private": true,
  "scripts": {
    "package": "ncc build --source-map --license licenses.txt -m -q",
    "test": "jest --coverage --silent=false --verbose false",
    "build": "tsc"
  },
  "type": "commonjs",
  "devDependencies": {
    "@swc/core": "1.3.11",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.1",
    "@vercel/ncc": "^0.36.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.4",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "appstore-connect-jwt-generator-core": "link:../../package"
  },
  "jest": {
    "testEnvironment": "node",
    "clearMocks": true,
    "extensionsToTreatAsEsm": [
      ".ts",
      ".cts"
    ],
    "moduleFileExtensions": [
      "cts",
      "ts",
      "js",
      "cjs",
      "json",
      "node"
    ],
    "testMatch": [
      "**/__tests__/*.test.ts",
      "**/__tests__/*.test.cts"
    ],
    "transform": {
      "^.+\\.c?(t|j)sx?$": [
        "ts-jest/legacy"
      ]
    },
    "modulePaths": [
      "<rootDir>/src",
      "<rootDir>/dist",
      "<rootDir>/node_modules"
    ],
    "verbose": true
  },
  "engines": {
    "npm": "use pnpm please!",
    "yarn": "use pnpm please!",
    "pnpm": ">=8",
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@9.1.4+sha512.9df9cf27c91715646c7d675d1c9c8e41f6fce88246f1318c1aa6a1ed1aeb3c4f032fcdf4ba63cc69c4fe6d634279176b5358727d8f2cc1e65b65f43ce2f8bfb0"
}
