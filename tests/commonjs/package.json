{
  "name": "commonjs",
  "version": "1.0.0",
  "main": "lib/main.cjs",
  "license": "MIT",
  "private": true,
  "scripts": {
    "package": "ncc build --source-map --license licenses.txt -m -q",
    "test": "jest --coverage --silent=false --verbose false",
    "build": "tsc"
  },
  "type": "commonjs",
  "devDependencies": {
    "@swc/core": "1.3.11",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.11",
    "@vercel/ncc": "^0.36.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.3",
    "typescript": "^5.5.3"
  },
  "dependencies": {
    "appstore-connect-jwt-generator-core": "link:../../package"
  },
  "jest": {
    "testEnvironment": "node",
    "clearMocks": true,
    "extensionsToTreatAsEsm": [
      ".ts",
      ".cts"
    ],
    "moduleFileExtensions": [
      "cts",
      "ts",
      "js",
      "cjs",
      "json",
      "node"
    ],
    "testMatch": [
      "**/__tests__/*.test.ts",
      "**/__tests__/*.test.cts"
    ],
    "transform": {
      "^.+\\.c?(t|j)sx?$": [
        "ts-jest/legacy"
      ]
    },
    "modulePaths": [
      "<rootDir>/src",
      "<rootDir>/dist",
      "<rootDir>/node_modules"
    ],
    "verbose": true
  },
  "engines": {
    "npm": "use pnpm please!",
    "yarn": "use pnpm please!",
    "pnpm": ">=8",
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@9.6.0+sha512.38dc6fba8dba35b39340b9700112c2fe1e12f10b17134715a4aa98ccf7bb035e76fd981cf0bb384dfa98f8d6af5481c2bef2f4266a24bfa20c34eb7147ce0b5e"
}
