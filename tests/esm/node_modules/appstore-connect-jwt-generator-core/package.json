{
  "name": "appstore-connect-jwt-generator-core",
  "version": "1.1.14",
  "author": "poad",
  "license": "MIT",
  "private": false,
  "homepage": "https://github.com/poad/appstore-connect-jwt-generator-core#readme",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/poad/appstore-connect-jwt-generator-core.git"
  },
  "keywords": [
    "jwt",
    "appstore"
  ],
  "type": "commonjs",
  "exports": {
    ".": {
      "import": "./esm/index.mjs",
      "require": "./lib/index.cjs",
      "default": "./esm/index.mjs"
    }
  },
  "module": "./esm/index.mjs",
  "types": "@types/index.d.ts",
  "main": "./lib/index.cjs",
  "scripts": {
    "clean": "rimraf dist",
    "cjs": "swc --config-file .swcrc-cjs --delete-dir-on-start --extensions .cts src/cjs -o lib/index.cjs",
    "esm": "swc --config-file .swcrc-esm --delete-dir-on-start --extensions .mts src/esm -o esm/index.mjs",
    "types": "tsc -p tsconfig-types.json",
    "build": "yarn clean && yarn cjs && yarn esm && yarn types",
    "package": "echo Skip the packaging process",
    "dev": "ts-node src/index.ts",
    "lint": "eslint src/esm/**/*.mts src/esm/*.mts src/cjs/**/*.cts src/cjs/*.cts src/*.ts",
    "test": "jest --coverage --passWithNoTests"
  },
  "devDependencies": {
    "@swc/cli": "*",
    "@swc/core": "*",
    "@swc/jest": "*",
    "@types/node": "*",
    "@typescript-eslint/eslint-plugin": "*",
    "@typescript-eslint/parser": "*",
    "chokidar": "*",
    "enhanced-resolve": "*",
    "eslint": "*",
    "jest": "*",
    "prettier": "*",
    "rimraf": "*",
    "ts-node": "*",
    "typescript": "*"
  },
  "dependencies": {
    "@types/jsonwebtoken": "*",
    "got": ">=13.0.0",
    "jsonwebtoken": "*",
    "log4js": "*",
    "regenerator-runtime": "*"
  },
  "resolutions": {
    "semver-regex": ">=3.1.3",
    "got": ">=11.8.5"
  },
  "engines": {
    "node": ">=14.13.1 || >=16.0.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "@swc/jest"
      ]
    }
  }
}
