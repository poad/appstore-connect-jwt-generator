{
  "name": "commonjs",
  "version": "1.0.0",
  "main": "esm/main.mjs",
  "license": "MIT",
  "private": true,
  "scripts": {
    "package": "ncc build --source-map --license licenses.txt -m -q --target es2021",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --coverage --silent=false --verbose false",
    "pretest": "pnpm build",
    "build": "tsc"
  },
  "type": "module",
  "devDependencies": {
    "@swc/core": "1.3.11",
    "@swc/jest": "^0.2.36",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.1",
    "@vercel/ncc": "^0.36.1",
    "jest": "^29.7.0",
    "source-map-support": "^0.5.21",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "appstore-connect-jwt-generator-core": "link:../../package"
  },
  "jest": {
    "testEnvironment": "node",
    "clearMocks": true,
    "extensionsToTreatAsEsm": [
      ".ts",
      ".cts",
      ".mts"
    ],
    "moduleFileExtensions": [
      "cts",
      "ts",
      "mts",
      "js",
      "cjs",
      "mjs",
      "json",
      "node"
    ],
    "testMatch": [
      "**/__tests__/*.test.ts",
      "**/__tests__/*.test.cts",
      "**/__tests__/*.test.mts"
    ],
    "transform": {
      "^.+\\.m?(t|j)sx?$": [
        "@swc/jest",
        {
          "jsc": {
            "parser": {
              "syntax": "typescript",
              "tsx": false,
              "decorators": true,
              "dynamicImport": true
            },
            "minify": {
              "compress": {
                "unused": true
              },
              "mangle": true
            },
            "target": "es2021",
            "transform": {
              "optimizer": {
                "jsonify": {
                  "minCost": 0
                }
              }
            }
          },
          "minify": true,
          "sourceMaps": true,
          "module": {
            "type": "es6",
            "strict": true,
            "strictMode": true,
            "lazy": true,
            "noInterop": false
          }
        }
      ]
    },
    "modulePaths": [
      "<rootDir>/src",
      "<rootDir>/ems",
      "<rootDir>/dist",
      "<rootDir>/node_modules"
    ],
    "verbose": true
  },
  "engines": {
    "npm": "use pnpm please!",
    "yarn": "use pnpm please!",
    "pnpm": ">=8.0.0",
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@9.1.4+sha512.9df9cf27c91715646c7d675d1c9c8e41f6fce88246f1318c1aa6a1ed1aeb3c4f032fcdf4ba63cc69c4fe6d634279176b5358727d8f2cc1e65b65f43ce2f8bfb0"
}
